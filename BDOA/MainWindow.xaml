        </Grid>
        <Grid x:Name="Tt6" HorizontalAlignment="Left" Height="90" Margin="222,354,-401,-444" VerticalAlignment="Top" Width="179" Visibility="Hidden">
            <Rectangle Fill="#cc000000" RadiusY="7" RadiusX="7"/>

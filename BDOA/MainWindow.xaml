        </Grid>
        <Grid x:Name="Tt8" HorizontalAlignment="Left" Height="147" Margin="222,354,-508,-501" VerticalAlignment="Top" Width="286" Visibility="Hidden">
            <Rectangle Fill="#cc000000" RadiusY="7" RadiusX="7"/>

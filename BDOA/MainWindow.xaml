        </Grid>
        <Grid x:Name="Tt5" HorizontalAlignment="Left" Height="98" Margin="222,305,-426,-403" VerticalAlignment="Top" Width="204" Visibility="Hidden">
            <Rectangle Fill="#cc000000" RadiusY="7" RadiusX="7"/>
